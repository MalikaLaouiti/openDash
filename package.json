    "@react-three/fiber": "^9.2.0",
    "chart": "^0.1.2",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.525.0",
    "motion": "^12.23.11",
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
