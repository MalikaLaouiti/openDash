{
  "name": "open-dash",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:next\"",
    "dev:server": "cross-env NODE_ENV=development nodemon server/server.mjs",
    "dev:next": "cross-env NODE_ENV=development next dev",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node server/server.mjs",
    "start:local": "concurrently \"npm run start:server\" \"npm run start:next\"",
    "start:server": "cross-env NODE_ENV=production node server/server.mjs",
    "start:next": "cross-env NODE_ENV=production next start",
    "lint": "next lint"
  },
